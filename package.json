{
  "name": "ai-qa-engineer",
  "version": "2.1.0",
  "description": "AI-powered QA testing. Use as CLI or integrate with your Playwright tests for unified reporting.",
  "main": "src/analyze.js",
  "types": "src/types.d.ts",
  "bin": {
    "ai-qa": "src/index.js"
  },
  "exports": {
    ".": {
      "require": "./src/analyze.js",
      "import": "./src/analyze.js",
      "types": "./src/types.d.ts"
    },
    "./cli": "./src/index.js"
  },
  "keywords": [
    "qa",
    "testing",
    "playwright",
    "claude",
    "openai",
    "gpt",
    "gemini",
    "ai",
    "automation",
    "github-actions"
  ],
  "author": "Tyler James Bridges",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyler-james-bridges/ai-qa-engineer"
  },
  "bugs": {
    "url": "https://github.com/tyler-james-bridges/ai-qa-engineer/issues"
  },
  "homepage": "https://github.com/tyler-james-bridges/ai-qa-engineer",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint scripts/ src/",
    "lint:fix": "eslint scripts/ src/ --fix",
    "test": "node --test scripts/**/*.test.js",
    "test:watch": "node --test --watch scripts/**/*.test.js",
    "install:browsers": "playwright install chromium",
    "visual:compare": "node scripts/visual-regression.cjs compare",
    "visual:update-baseline": "node scripts/visual-regression.cjs update-baseline"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@google/generative-ai": "^0.21.0",
    "openai": "^4.77.0",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.48.0",
    "pngjs": "^7.0.0"
  },
  "peerDependencies": {
    "playwright": ">=1.40.0",
    "@playwright/test": ">=1.40.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    },
    "@playwright/test": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  }
}
